/*
 Функция Pal проверяет, является ли строка палиндромом.
 Функция Inv инвертирует строку.
 Суперкомпилятору предлагается верифицировать факт, что строка+ее инверсия есть палиндром.
*/

$ENTRY Go {
	e.x = <Pal e.x <Inv e.x>>;
}

Pal {
	 = 'T';
	t.1 = 'T';
	t.1 e.x t.1 = <Pal e.x>;
	e.Z = 'F';
}

Inv {
    = ;
    t.1 e.1 = <Inv e.1> t.1;
}
